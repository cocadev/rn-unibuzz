{
    "expo": {
        "name": "Unibuzz",
        "description": "This project is really great.",
        "slug": "unibuzz",
        "privacy": "unlisted",
        "platforms": ["ios", "android"],
        "sdkVersion": "32.0.0",
        "version": "1.0.0",
        "orientation": "portrait",
        "primaryColor": "#cccccc",
        "icon": "./assets/images/icon.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "loading": {
            "icon": "./assets/images/loading-icon.png",
            "hideExponentText": false
        },
        "packagerOpts": {
            "assetExts": [
                "ttf",
                "mp4",
                "otf",
                "xml"
            ]
        },
        "android": {
            "package": "com.wang"
        },
        "ios": {
            "supportsTablet": true
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ]
    }
}